<p id="notice"><%= notice %></p>

<h2>Listing Locations: Victoria State</h2>

<table class="table table-striped table-bordered table-condensed">

  <thead>
    <tr class="success">
    <th>ID of the location</th><th>Longitude</th><th>Latitude</th>
    </tr>
  </thead>
  <tbody>
  <%Location.all.each do |l|%>
    <tr>
      <td><%=l.location_id%></td>
      <td><%=l.long%></td>
      <td><%=l.lat%></td>
    </tr>
  <%end%>
  </tbody>
</table>
